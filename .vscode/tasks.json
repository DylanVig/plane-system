{
	"version": "2.0.0",
	"tasks": [
		{
			"type": "cargo",
			"command": "build",
			"problemMatcher": [
				"$rustc"
			],
			"group": {
				"kind": "build",
				"isDefault": true
			},
			"label": "rust: cargo build",
			"args": [
				"--target=aarch64-unknown-linux-gnu"
			]
		},
		{
			"type": "shell",
			"command": "scp target/aarch64-unknown-linux-gnu/debug/plane-system ubuntu@68.175.147.222:/home/ubuntu/plane-system",
			"label": "copy to pi",
			"problemMatcher": []
		}
	]
}
